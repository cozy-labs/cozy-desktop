{
  "name": "cozy-desktop",
  "version": "0.16.0",
  "description": "File Synchronization Client for Cozy Cloud",
  "homepage": "https://docs.cozy.io/en/mobile/desktop.html",
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozycloud.cc/)",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy-labs/cozy-desktop/issues"
  },
  "main": "./lib/app.js",
  "bin": {
    "cozy-desktop": "bin/cozy-desktop"
  },
  "repository": "git://github.com/cozy-labs/cozy-desktop.git",
  "logo": "https://raw.githubusercontent.com/cozy-labs/cozy-desktop/master/gui/app/images/icon.png",
  "keywords": [
    "desktop",
    "data",
    "sync",
    "files",
    "cozy"
  ],
  "files": [
    "bin/cozy-desktop",
    "doc/",
    "lib/",
    "CHANGELOG.md",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "bootstrap": "docker exec -it cozydesktop_stack_1 bootstrap-cozy-desktop.sh",
    "build": "babel --source-maps --out-dir lib/ src/",
    "clean": "rm -rf lib/ && rm -rf tmp/",
    "dev-storage": "yarn stack -- tree -aC /storage/dev.cozy.tools:8080",
    "eslint": "./node_modules/.bin/eslint './src/**/*.js' './test/**/*.js'",
    "fauxton": "babel-node ./src/bin/fauxton.js",
    "flow": "flow status --quiet --show-all-errors",
    "lint": "npm run eslint && npm run flow",
    "mocha": "env-cmd .env.test mocha",
    "repl": "cross-env COZY_DESKTOP_HEARTBEAT=5000 babel-node ./src/bin/repl.js",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "start": "cross-env COZY_DESKTOP_HEARTBEAT=5000 babel-node ./src/bin/cli.js",
    "stack": "docker exec -it cozydesktop_stack_1",
    "test": "installed-check && npm run test-unit && npm run test-integration",
    "test-coverage": "installed-check && npm run test-unit-coverage && npm run test-integration",
    "test-integration": "cross-env NODE_ENV=test mocha test/integration/v3",
    "test-unit": "yarn mocha -- test/unit/",
    "test-unit-coverage": "cross-env NODE_ENV=test nyc mocha test/unit/",
    "watch": "npm run build -- --watch"
  },
  "dependencies": {
    "async": "^2.1.5",
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "btoa": "1.1.2",
    "bunyan": "^1.8.10",
    "chokidar": "1.6.1",
    "commander": "2.9.0",
    "cozy-client-js": "^0.1.11",
    "fs-extra": "^2.1.1",
    "isomorphic-fetch": "2.2.1",
    "lodash.clone": "4.5.0",
    "lodash.find": "4.6.0",
    "lodash.isequal": "4.5.0",
    "lodash.pick": "4.4.0",
    "micromatch": "2.3.11",
    "node-fetch": "1.6.3",
    "opn": "4.0.2",
    "pouchdb": "6.1.2",
    "pouchdb-find": "^0.10.5",
    "progress": "^2.0.0",
    "read": "1.0.7",
    "readdirp": "2.1.0",
    "request-json-light": "0.5.25",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-autobind-class-methods": "^5.0.1",
    "babel-plugin-istanbul": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2016": "^6.22.0",
    "babel-preset-es2017": "^6.22.0",
    "cross-env": "^4.0.0",
    "del": "^2.2.2",
    "env-cmd": "^5.0.0",
    "eslint": "^3.17.1",
    "eslint-config-standard": "^8.0.0",
    "eslint-plugin-flowtype": "^2.30.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "faker": "^4.1.0",
    "flow-bin": "^0.44.0",
    "flow-typed": "^2.0.0",
    "installed-check": "^2.1.1",
    "istanbul": "^0.4.5",
    "jsverify": "^0.8.0",
    "lodash.uniq": "^4.5.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2",
    "pouchdb-server": "^2.3.4",
    "should": "^11.2.1",
    "should-sinon": "^0.0.5",
    "sinon": "^2.0.0",
    "sinon-stub-promise": "^4.0.0",
    "source-map-support": "^0.4.11"
  }
}
