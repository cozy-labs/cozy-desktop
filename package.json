    "lodash.clone": "4.3.2",
    "lodash.find": "4.2.1",
    "lodash.isequal": "4.1.3",
    "lodash.pick": "4.1.1",
    "micromatch": "2.3.7",
    "mime": "1.3.4",
    "node-uuid": "1.4.7",
    "path-extra": "3.0.0",
    "pouchdb": "5.3.1",
    "printit": "0.1.18",
    "progress": "1.1.8",
    "read": "1.0.7",
    "readdirp": "2.0.0",
    "request-json-light": "0.5.22"
